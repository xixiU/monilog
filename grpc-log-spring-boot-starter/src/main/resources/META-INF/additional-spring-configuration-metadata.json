{
  "properties": [
    {
      "name": "monitor.log.grpc.enable",
      "type": "java.lang.Boolean",
      "sourceType": "com.jiduauto.log.grpc.GrpcMonitorLogConfiguration",
      "defaultValue": true,
      "description": "开启grpc监控,默认会开启server与client"
    },
    {
      "name": "monitor.log.grpc.server.enable",
      "type": "java.lang.Boolean",
      "sourceType": "com.jiduauto.log.grpc.filter.GrpcLogPrintServerInterceptor",
      "defaultValue": true,
      "description": "开启grpc服务端监控"
    },
    {
      "name": "monitor.log.grpc.client.enable",
      "type": "java.lang.Boolean",
      "sourceType": "com.jiduauto.log.grpc.filter.GrpcLogPrintClientInterceptor",
      "defaultValue": true,
      "description": "开启grpc客户端监控"
    }
  ]
}